APPLICATION=../apps

DATA_PATH=~/.dockery/data

RUBY_VERSION=242

# Create below folder on your host machine with a write access
# from docker container
RUBY_BUNDLE_DIR=/private/var/bundle/2.4.2

RUBY_INSTALL_RAILS=true
RUBY_INSTALL_NODE=true
RUBY_RAILS_PORT=3000
RUBY_RAILS_VERSION=5.1.4

POSTGRES_USER=root
POSTGRES_DB=default
POSTGRES_PASSWORD=secret
POSTGRES_PORT=5432

MYSQL_DATABASE=default
MYSQL_USER=default
MYSQL_PASSWORD=secret
MYSQL_PORT=3306
MYSQL_ROOT_PASSWORD=root
MYSQL_TIMEZONE=UTC

DOCKER_SYNC_STRATEGY=native_osx
